{
	"name" : "Tcp Client Server(App ver)",
	"version" : "1.0",
	"manifest_version" : 2,
	"description" : "Tcp Client Server Test.",
	"permissions": [
		"tabs", "<all_urls>", "contextMenus"
	],
	"icons": { 
		"16": "image/icon16.png",
		"48": "image/icon48.png",
		"128": "image/icon128.png" },
	"content_scripts": [
		{
			"matches": ["http://*/*"],
			"js": ["js/jquery-1.9.1.js", "js/contentscript.js"],
			"all_frames": true
		}
	],
	"app": {
		"background": {
			"scripts": ["js/background.js"]
		}
	},
	"sockets": {
		"tcp": {
			"connect": "*"
		},
		"tcpServer": {
			"listen": "*"
		}
	}
}